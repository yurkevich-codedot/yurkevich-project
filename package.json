{
  "name": "static-site-webpack",
  "version": "1.0.0",
  "scripts": {
    "start": "webpack serve --no-client-overlay-warnings --open --port 3000",
    "watch": "webpack --mode development --watch",
    "dev": "webpack --mode development",
    "build": "webpack --mode production && prettier --print-width=120 --parser html --write dist/*.html",
    "format": "prettier --write **/*.{js,scss,html}",
    "post-update": "yarn upgrade --latest"
  },
  "dependencies": {
    "destyle.css": "^3.0.2"
  },
  "devDependencies": {
    "clean-webpack-plugin": "^4.0.0",
    "copy-webpack-plugin": "^11.0.0",
    "css-loader": "^6.7.1",
    "css-minimizer-webpack-plugin": "^4.0.0",
    "html-webpack-plugin": "^5.5.0",
    "mini-css-extract-plugin": "^2.6.0",
    "prettier": "^2.6.1",
    "raw-loader": "^4.0.2",
    "sass": "^1.49.10",
    "sass-loader": "^13.0.0",
    "terser-webpack-plugin": "^5.3.1",
    "webpack": "^5.70.0",
    "webpack-cli": "^4.9.2",
    "webpack-dev-server": "^4.7.4"
  },
  "repository": "https://github.com/filimonov-codedot/static-site-webpack",
  "author": "filimonov-codedot <filimonov.codedot@gmail.com>",
  "license": "MIT"
}
